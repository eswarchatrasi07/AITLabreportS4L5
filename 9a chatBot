import openai
openai.api_key = "sk-T0ixcMgS8icuB5RepAAT3BlbFJTJd0GtJXcA99EXobjsCl"
completion = openai.ChatCompletion.create(
    model = "gpt-3.5-turbo",
    messages = [{"role" : "user", "content" : "Give me 3 ideas that I could build using OpenAI"}]
)

print(completion.choices[0].message["content"])
